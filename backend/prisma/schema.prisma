generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  email     String  @unique
  password  String
  fullName  String?
  username  String?
  phone     String?
  address   String?
  createdAt DateTime @default(now())
}
